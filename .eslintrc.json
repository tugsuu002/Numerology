{
    "env": {
      "browser": true, 
      "es6": true,
      "node": true
    },  
    "extends": [
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "spread": true,
        "restParams": true  
      },
      "sourceType": "module"
    }
    
  }
